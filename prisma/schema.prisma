generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Morador {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  name    String?
  street  String
  house   Int
  status  String

}